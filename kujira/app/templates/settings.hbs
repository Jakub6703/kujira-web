<h2>Users</h2>

<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Type</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
      {{#each model as |user|}}
          <tr>
            <td> {{user.id}} </td>
            <td> {{user.name}} </td>
            <td> {{user.userType}} </td>
            <td><button type="button" class="btn btn-default"> save </button></td>
          </tr>
      {{/each}}
  </tbody>
</table>
